{% for link in links %}
	<li class="c-list-link">
		{% if link.sub_title is defined %}
			<p class="c-list-link__sub-title">{{ link.sub_title }}</p>
		{% endif %}
		{% if link.text is defined %}
			<a href="{{ link.href }}" class="c-list-link__anchor">{{ link.text }}</a>
		{% endif %}
	</li>
{% endfor %}
